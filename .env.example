# DBF 檔案路徑
# 本機開發環境
DBF_FILE_PATH=D:/programing/github/anchia_clinic_monitor/data/CO05T.DBF

# Docker 環境（透過 Volume 掛載）
# DBF_FILE_PATH=/app/data/CO05T.DBF

# 檔案監控間隔（毫秒）
WATCH_INTERVAL=3000

# 後端埠號
PORT=3001

# CORS 設定（生產環境建議限制來源）
CORS_ORIGIN=*

# 隱私模式預設值
DEFAULT_PRIVACY_MODE=false

# 顯示退掛人員預設值
DEFAULT_SHOW_CANCELLED=false

# Node 環境
NODE_ENV=development
